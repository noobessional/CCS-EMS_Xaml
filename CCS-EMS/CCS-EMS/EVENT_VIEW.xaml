                                <Border Width="60" Height="10" Background="#EFEFEF" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="30 5 0 0">
                                    <TextBlock Text="Event Details" Foreground="#FF5B5B5B" FontSize="8" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </Border>
                            </Grid>
                        </StackPanel>
                    </Grid>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                        <Button x:Name="EDIT_EVENT_CANCEL_BUTTON" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="5" Style="{StaticResource CANCEL_BUTTON}" Width="75" Height="30" Content="Cancel" MouseEnter="EDIT_EVENT_CANCEL_BUTTON_MouseEnter" MouseLeave="EDIT_EVENT_CANCEL_BUTTON_MouseLeave" Click="EDIT_EVENT_CANCEL_BUTTON_Click"/>
